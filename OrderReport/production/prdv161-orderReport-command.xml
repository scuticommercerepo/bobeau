<?xml version="1.0" encoding="UTF-8"?>
<marketlive xmlns="http://marketlive.com/integration/xmlbean">
  <export>
		<command type="findByCriteria">
			<findByCriteriaParameters>
				<entityType>order</entityType>
				<locale>en_US</locale>
				<hql>from Order o where o.dateCreated > (sysdate - 1)</hql>
			</findByCriteriaParameters>
		</command>
  </export>
</marketlive>
